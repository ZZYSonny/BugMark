{
  "name": "bookmarkfordebugging",
  "displayName": "BookmarkForDebugging",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bookmarkfordebugging.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "esbuild src/extension.ts --outdir=out --bundle --platform=node --packages=external --sourcemap=external",
    "watch": "esbuild src/extension.ts --outdir=out --bundle --platform=node --packages=external --sourcemap=external --watch"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3",
    "esbuild": "^0.19.11"
  }
}